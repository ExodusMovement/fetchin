{
  "license": "MIT",
  "name": "fetchin",
  "description": "Fetch helpers.",
  "version": "0.0.15",
  "main": "dist/index.js",
  "react-native": "dist/rn.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonaye/fetchin.git"
  },
  "optionalDependencies": {
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.12.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "23.1.0"
  },
  "scripts": {
    "lint": "eslint src",
    "pretest": "NODE_ENV=production babel src/lib --out-dir dist",
    "test": "jest src/test",
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "NODE_ENV=production babel src/lib --out-file dist/index.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "verbose": true
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "babel-runtime": "6.26.0"
  }
}
