{
  "name": "fetchin",
  "description": "Fetch helpers.",
  "version": "0.0.10",
  "main": "lib/index.js",
  "react-native": "lib/rn.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonaye/fetchin.git"
  },
  "optionalDependencies": {
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-0": "6.24.1",
    "isomorphic-fetch": "^2.2.1",
    "nodemon": "1.17.2"
  },
  "scripts": {
    "start": "yarn build --watch & nodemon lib/tests.js",
    "build":
      "rm -r -f lib && mkdir -p lib && BABEL_ENV=production babel src --out-dir lib"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": { "node": "6.9.0" },
          "browsers": ["> 1%", "last 2 versions"]
        }
      ],
      "stage-0"
    ]
  }
}
