{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": 0,
    "comma-dangle": [2, "never"],
    "function-paren-newline": 0,
    "implicit-arrow-linebreak": 0,
    "import/no-extraneous-dependencies": [0, { "optionalDependencies": true }],
    "no-return-assign": 0,
    "object-curly-newline": 0
  }
}
